[texml]
use_xetex=1
# extra_lib=/work/github/AmerMathSoc/texml-amsmetadata/lib/perl
extra_lib=$TEXML_ROOT/external/texml-amsmetadata/lib/perl
pre_texinputs=$TEXML_ROOT/lib/texmf/tex/latex/texml

[TeX::Utils::SVG]
dvi_engine=pdflatex --progname=prdlatex -output-format dvi
pdf_engine=xelatex --progname=prdxelatex
dvips=dvips
pdfcrop=pdfcrop
pdf2svg=pdf2svg
ps2pdf=ps2pdf

[TeX::KPSE]
program_name=pdflatex
kpsewhich=kpsewhich

[TeX::FMT::File]
tlyear=2016
