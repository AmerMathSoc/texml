[texml]
use_xetex=0
extra_lib=/ams/texmf/lib/perl

[TeX::Interpreter]
engine=prdlatex

[TeX::Utils::SVG]
dvi_engine=/ams/texmf/archive/texlive2016/bin/x86_64-linux/pdflatex \
    --progname=prdlatex -output-format dvi
pdf_engine=/ams/texmf/archive/texlive2016/bin/x86_64-linux/xelatex \
    --progname=prdxelatex
dvips=/ams/texmf/archive/texlive2016/bin/x86_64-linux/dvips
pdfcrop=/ams/texmf/archive/texlive2016/bin/x86_64-linux/pdfcrop
pdf2svg=/usr/bin/pdf2svg
ps2pdf=/ams/texmf/bin/distill -pd +AMSPress
