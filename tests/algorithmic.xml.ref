  <body id="ltxid2">
    <sec disp-level="1" id="ltxid3" specific-use="section">
      <p>hello, world</p>
    </sec>
    <sec disp-level="1" id="ltxid4" specific-use="section">
      <label>1</label>
      <title>Basic forms</title>
      <sec disp-level="2" id="ltxid5" specific-use="subsection">
        <label>1.1</label>
        <title>The Simple Statement</title>
        <alg:algorithm>
          <alg:line>
            <alg:state>
              <inline-formula content-type="math/tex">
                <tex-math>S \leftarrow O</tex-math>
              </inline-formula>
            </alg:state>
          </alg:line>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid6" specific-use="subsection">
        <label>1.2</label>
        <title>The body</title>
        <alg:algorithm>
          <alg:body>
            <alg:line>
              <alg:state>something</alg:state>
            </alg:line>
          </alg:body>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid7" specific-use="subsection">
        <label>1.3</label>
        <title>The <italic toggle="yes">if-then-else</italic> Statement</title>
        <alt-title>The if-then-else Statement</alt-title>
        <alg:algorithm>
          <alg:if>
            <alg:condition>
              <alg:line>some condition is true</alg:line>
            </alg:condition>
            <alg:block>
              <alg:line>
                <alg:state>do some processing</alg:state>
              </alg:line>
            </alg:block>
            <alg:elsif>
              <alg:condition>
                <alg:line>some other condition is true</alg:line>
              </alg:condition>
              <alg:block>
                <alg:line>
                  <alg:state>do some different processing</alg:state>
                </alg:line>
              </alg:block>
            </alg:elsif>
            <alg:elsif>
              <alg:condition>
                <alg:line>some even more bizarre condition is met</alg:line>
              </alg:condition>
              <alg:block>
                <alg:line>
                  <alg:state>do something else</alg:state>
                </alg:line>
              </alg:block>
            </alg:elsif>
            <alg:else>
              <alg:block>
                <alg:line>
                  <alg:state>do the default actions</alg:state>
                </alg:line>
              </alg:block>
            </alg:else>
          </alg:if>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid8" specific-use="subsection">
        <label>1.4</label>
        <title>The <italic toggle="yes">for</italic> Loop</title>
        <alt-title>The for Loop</alt-title>
        <alg:algorithm>
          <alg:for>
            <alg:condition>
              <alg:line>
                <inline-formula content-type="math/tex">
                  <tex-math>i=0</tex-math>
                </inline-formula> to <inline-formula content-type="math/tex"><tex-math>10</tex-math></inline-formula></alg:line>
            </alg:condition>
            <alg:block>
              <alg:line>
                <alg:state>carry out some processing</alg:state>
              </alg:line>
            </alg:block>
          </alg:for>
        </alg:algorithm>
        <alg:algorithm>
          <alg:forall>
            <alg:condition>
              <alg:line>
                <inline-formula content-type="math/tex">
                  <tex-math>i</tex-math>
                </inline-formula> such that <inline-formula content-type="math/tex"><tex-math>0\leq i\leq 10</tex-math></inline-formula></alg:line>
            </alg:condition>
            <alg:block>
              <alg:line>
                <alg:state>carry out some processing</alg:state>
              </alg:line>
            </alg:block>
          </alg:forall>
        </alg:algorithm>
        <alg:algorithm>
          <alg:for>
            <alg:condition>
              <alg:line>
                <inline-formula content-type="math/tex">
                  <tex-math>i=0</tex-math>
                </inline-formula> <bold>to</bold> <inline-formula content-type="math/tex"><tex-math>10</tex-math></inline-formula></alg:line>
            </alg:condition>
            <alg:block>
              <alg:line>
                <alg:state>carry out some processing</alg:state>
              </alg:line>
            </alg:block>
          </alg:for>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid9" specific-use="subsection">
        <label>1.5</label>
        <title>The <italic toggle="yes">while</italic> Loop</title>
        <alt-title>The while Loop</alt-title>
        <alg:algorithm>
          <alg:while>
            <alg:condition>
              <alg:line>some condition holds</alg:line>
            </alg:condition>
            <alg:block>
              <alg:line>
                <alg:state>carry out some processing</alg:state>
              </alg:line>
            </alg:block>
          </alg:while>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid10" specific-use="subsection">
        <label>1.6</label>
        <title>The <italic toggle="yes">repeat-until</italic> Loop</title>
        <alt-title>The repeat-until Loop</alt-title>
        <alg:algorithm>
          <alg:repeat>
            <alg:block>
              <alg:line>
                <alg:state>carry out some processing</alg:state>
              </alg:line>
            </alg:block>
            <alg:until>
              <alg:condition>
                <alg:line>some condition is met</alg:line>
              </alg:condition>
              <alg:comment>some condition is met</alg:comment>
            </alg:until>
          </alg:repeat>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid11" specific-use="subsection">
        <label>1.7</label>
        <title>The Infinite Loop</title>
        <alg:algorithm>
          <alg:loop>
            <alg:block>
              <alg:line>
                <alg:state>this processing will be repeated forever</alg:state>
              </alg:line>
            </alg:block>
          </alg:loop>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid12" specific-use="subsection">
        <label>1.8</label>
        <title>The Precondition</title>
        <alg:algorithm>
          <alg:require>
            <inline-formula content-type="math/tex">
              <tex-math>x \neq 0</tex-math>
            </inline-formula> and <inline-formula content-type="math/tex"><tex-math>n \geq 0</tex-math></inline-formula></alg:require>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid13" specific-use="subsection">
        <label>1.9</label>
        <title>The Postcondition</title>
        <alg:algorithm>
          <alg:ensure>
            <inline-formula content-type="math/tex">
              <tex-math>x \neq 0</tex-math>
            </inline-formula> and <inline-formula content-type="math/tex"><tex-math>n \geq 0</tex-math></inline-formula></alg:ensure>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid14" specific-use="subsection">
        <label>1.10</label>
        <title>Globals</title>
        <alg:algorithm>
          <alg:line>
            <alg:globals>
              <inline-formula content-type="math/tex">
                <tex-math>x</tex-math>
              </inline-formula>, <inline-formula content-type="math/tex"><tex-math>y</tex-math></inline-formula></alg:globals>
          </alg:line>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid15" specific-use="subsection">
        <label>1.11</label>
        <title>Inputs</title>
        <alg:algorithm>
          <alg:inputs>
            <alg:line>
              <alg:state>
                <inline-formula content-type="math/tex">
                  <tex-math>x</tex-math>
                </inline-formula>, <inline-formula content-type="math/tex"><tex-math>y</tex-math></inline-formula></alg:state>
            </alg:line>
          </alg:inputs>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid16" specific-use="subsection">
        <label>1.12</label>
        <title>Outputs</title>
        <alg:algorithm>
          <alg:outputs>
            <alg:line>
              <alg:state>
                <inline-formula content-type="math/tex">
                  <tex-math>x</tex-math>
                </inline-formula>, <inline-formula content-type="math/tex"><tex-math>y</tex-math></inline-formula></alg:state>
            </alg:line>
          </alg:outputs>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid17" specific-use="subsection">
        <label>1.13</label>
        <title>Returning Values</title>
        <alg:algorithm>
          <alg:line>
            <alg:state>
              <bold>return</bold> <inline-formula content-type="math/tex"><tex-math>(x+y)/2</tex-math></inline-formula></alg:state>
          </alg:line>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid18" specific-use="subsection">
        <label>1.14</label>
        <title>Printing Messages</title>
        <alg:algorithm>
          <alg:line>
            <alg:state>
              <bold>print</bold> <monospace>“Hello, World!”</monospace></alg:state>
          </alg:line>
        </alg:algorithm>
      </sec>
      <sec disp-level="2" id="ltxid19" specific-use="subsection">
        <label>1.15</label>
        <title>Comments</title>
        <alg:algorithm>
          <alg:line>
            <alg:state>do something<alg:comment>this is a comment</alg:comment></alg:state>
          </alg:line>
        </alg:algorithm>
      </sec>
    </sec>
    <sec disp-level="1" id="ltxid20" specific-use="section">
      <label>2</label>
      <title>Some longer examples</title>
      <alg:algorithm>
        <alg:line>
          <alg:state>
            <inline-formula content-type="math/tex">
              <tex-math>a \leftarrow 1</tex-math>
            </inline-formula>
          </alg:state>
        </alg:line>
        <alg:if>
          <alg:condition>
            <alg:line>
              <inline-formula content-type="math/tex">
                <tex-math>a</tex-math>
              </inline-formula> is even</alg:line>
          </alg:condition>
          <alg:block>
            <alg:line>
              <alg:state>
                <bold>print</bold> “<inline-formula content-type="math/tex"><tex-math>a</tex-math></inline-formula> is even”</alg:state>
            </alg:line>
          </alg:block>
          <alg:elsif>
            <alg:condition>
              <alg:line>
                <inline-formula content-type="math/tex">
                  <tex-math>a</tex-math>
                </inline-formula> is odd</alg:line>
            </alg:condition>
            <alg:block>
              <alg:line>
                <alg:state>
                  <bold>print</bold> “<inline-formula content-type="math/tex"><tex-math>a</tex-math></inline-formula> is odd”</alg:state>
              </alg:line>
            </alg:block>
          </alg:elsif>
          <alg:else>
            <alg:block>
              <alg:line>
                <alg:state>
                  <bold>print</bold> “<inline-formula content-type="math/tex"><tex-math>a</tex-math></inline-formula> is really weird”</alg:state>
              </alg:line>
            </alg:block>
          </alg:else>
        </alg:if>
      </alg:algorithm>
      <alg:algorithm>
        <alg:require>
          <inline-formula content-type="math/tex">
            <tex-math>n \geq 0</tex-math>
          </inline-formula>
        </alg:require>
        <alg:ensure>
          <inline-formula content-type="math/tex">
            <tex-math>y = x^n</tex-math>
          </inline-formula>
        </alg:ensure>
        <alg:state>
          <inline-formula content-type="math/tex">
            <tex-math>y \leftarrow 1</tex-math>
          </inline-formula>
        </alg:state>
        <alg:state>
          <inline-formula content-type="math/tex">
            <tex-math>X \leftarrow x</tex-math>
          </inline-formula>
        </alg:state>
        <alg:state>
          <inline-formula content-type="math/tex">
            <tex-math>N \leftarrow n</tex-math>
          </inline-formula>
        </alg:state>
        <alg:while>
          <alg:condition>
            <alg:line>
              <inline-formula content-type="math/tex">
                <tex-math>N \neq 0</tex-math>
              </inline-formula>
            </alg:line>
          </alg:condition>
          <alg:block>
            <alg:if>
              <alg:condition>
                <alg:line>
                  <inline-formula content-type="math/tex">
                    <tex-math>N</tex-math>
                  </inline-formula> is even</alg:line>
              </alg:condition>
              <alg:block>
                <alg:line>
                  <alg:state>
                    <inline-formula content-type="math/tex">
                      <tex-math>X \leftarrow X \times X</tex-math>
                    </inline-formula>
                  </alg:state>
                </alg:line>
                <alg:line>
                  <alg:state>
                    <inline-formula content-type="math/tex">
                      <tex-math>N \leftarrow N / 2</tex-math>
                    </inline-formula>
                  </alg:state>
                </alg:line>
              </alg:block>
              <alg:else>
                <alg:block>
                  <alg:line>
                    <alg:state>
                      <inline-formula content-type="math/tex">
                        <tex-math>y \leftarrow y \times X</tex-math>
                      </inline-formula>
                    </alg:state>
                  </alg:line>
                  <alg:line>
                    <alg:state>
                      <inline-formula content-type="math/tex">
                        <tex-math>N \leftarrow N - 1</tex-math>
                      </inline-formula>
                    </alg:state>
                  </alg:line>
                </alg:block>
              </alg:else>
            </alg:if>
          </alg:block>
        </alg:while>
      </alg:algorithm>
    </sec>
  </body>
