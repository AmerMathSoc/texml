<!ENTITY % article-atts
            "%jats-common-atts;
             article-type
                        CDATA                             #IMPLIED
             specific-use
                        CDATA                             #IMPLIED
             xml:lang   NMTOKEN                           'en'
             %dtd-version;
             %XLINK.xmlns.attrib;
             %MATHML.xmlns.attrib;
             %ali.xmlns.attrib;
             %Schema.xmlns.attrib;
             %Schema.prefix;:noNamespaceSchemaLocation
                        CDATA          #IMPLIED"                    >

