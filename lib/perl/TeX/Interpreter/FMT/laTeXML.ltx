\def \DeclareTextFontCommand #1#2{%
    \DeclareRobustCommand#1[1]{%
        \ifmmode
            \nfss@text{#2##1}%
        \else
            \hmode@bgroup
                \text@command{##1}%
                #2\check@icl ##1\check@icr
            \expandafter\egroup
        \fi
    }%
}

\DeclareTextFontCommand{\textrm}{\rmfamily}
\DeclareTextFontCommand{\textsf}{\sffamily}
\DeclareTextFontCommand{\texttt}{\ttfamily}
