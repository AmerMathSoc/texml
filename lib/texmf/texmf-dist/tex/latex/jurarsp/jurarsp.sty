                \@for\rsp@list@tempa:=#1\do{%
                    \expandafter\gdef\csname\rsp@list@tempa\endcsname{}%
                }%
    }{%
                \rsp@info{Resetting citefirst on #3
                     for Section \thesection}%
                \@for\rsp@list@tempa:=#1\do{%
                    \expandafter\gdef\csname\rsp@list@tempa\endcsname{}%
                }%
