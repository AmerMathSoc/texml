\unless\ifbxjy@ok
\bxjy@decl@family{mc}{\bxjy@sc ipxm-r-u}{CJKssub*gt/m/n}{CJKssub*gt/m/n}
\fi
\bxjy@check@alldefined{gtr,gtb,gtx}%
\unless\ifbxjy@ok 
\bxjy@decl@family{gt}{\bxjy@sc ipxg-r-u}{CJKssub*gt/m/n}{CJKssub*gt/m/n}
