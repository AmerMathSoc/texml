%% \bxjy@NEF/<sym>
\def\bxjy@do#1#2{%
  \def\bxjy@tmpb{#2}%
  \@for\bxjy@tmpa:=#1\do{%
    \cslet{bxjy@NEF/\bxjy@tmpa}\bxjy@tmpb}}
