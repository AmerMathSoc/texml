    \fi\fi\fi
   \ifx\next@ba[%
      \let\next@ba=\getk@bapg \if@pfvba\global\let\pfilled@ba=+\fi
   \else\ifx\next@ba(%
      \let\next@ba=\getkf@bapg \if@pfvba\global\let\pfilled@ba=+\fi
   \else\ifx\next@ba*%
      \let\next@ba=\xk@bapg
