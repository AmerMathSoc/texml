    \if@pfvba
     \ifx\isibidem@ba+\ifx\pfilled@ba+%
        \lessinfoerror@ba{page/folio}{[n]- or (n)}{\pref@ba}{\notprintlongpagefolio}{document}%
         \ifx\lockpevo@ba-%
