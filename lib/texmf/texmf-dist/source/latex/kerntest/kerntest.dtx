    \ifthenelse{\equal{\oldkerna}{}}{}{\color{newcolor}}%
  \else
    \color{newcolor}%
  \fi
  \ifx\@kernlenb\@minussign
    \ifthenelse{\equal{\oldkernb}{}}{}{\color{newcolor}}%
  \else
    \color{newcolor}%
  \fi
  \char\the\c@@slota%
  \ifx\@kernlena\@minussign
    \oldkerna
  \else
    \kern#2\psunit
  \fi
  \char\the\c@@slotb%
  \ifx\@kernlenb\@minussign
    \oldkernb
  \else
    \kern#4\psunit
  \fi
