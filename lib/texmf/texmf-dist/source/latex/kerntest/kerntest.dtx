% The first glyph is the referent glyph of the class.
% If the inner loop is executed the first time, set
% \cs{firstglyphinclass} to this value.
%    \begin{macrocode}
      \@tempswatrue
      \@forallinclass{\@tmpcls}{@tmpglyph}{%
        \if@tempswa
          \edef\firstglyphinclass{\@tmpglyph}%
          \@tempswafalse
        \else
