% See \LaTeX{} manual for their definition.
%    \begin{macrocode}
\setcounter{topnumber}{4}
\renewcommand\topfraction{.9}
\setcounter{bottomnumber}{4}
\renewcommand\bottomfraction{.7}
\setcounter{totalnumber}{10}
\renewcommand\textfraction{.1}
