    \def\@tempa{#1}%
    \ifx\@tempa\empty\def\@tempa{0}\fi
    \ifx\@tempa\space\def\@tempa{0}\fi
    \ifnum\ifnum\@tempa<\z@-\fi\@tempa>#3\@badlinearg \fi}
  \def\@badlinearg{\PackageError
    {pict2e}{Bad \protect\line\space or \protect\vector\space argument}{}}
