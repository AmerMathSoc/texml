  \newcommand*\pIIe@checkslopeargs[3]{%
    \def\@tempa{#1}\expandafter\pIIe@checkslopearg\@tempa.:{#3}%
    \def\@tempa{#2}\expandafter\pIIe@checkslopearg\@tempa.:{#3}%
