%    \begin{macrocode}
  \def\@sline{%
    \begingroup
    \ifdim\@tempdima=\z@
      \ifdim\@tempdimb<\z@\@linelen-\@linelen\fi
      \@ydim=\@linelen
      \@xdim=\z@
    \else
