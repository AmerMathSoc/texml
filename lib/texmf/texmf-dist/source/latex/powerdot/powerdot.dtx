% makes the text font |\small|, but only for a4 portrait slides.
%    \begin{macrocode}
\def\pd@tempa#1{%
  \define@key[pd]{template}{#1}[]{%
    \ifpd@ifsetup\@namedef{pd@@#1}{##1}\fi
