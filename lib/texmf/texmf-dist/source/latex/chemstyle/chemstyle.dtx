\newcommand*\cst@phrase[1]{%
  \latin{#1}%
  \@ifnextchar.{}{%
    .%
    \cst@xspace
  }%
}
\newcommand*\cst@phrase@comma{%
  \ifcst@abbrcomma
    \expandafter\cst@phrase@with@comma
  \else
    \expandafter\cst@phrase
  \fi
}
