%    \cs{}\meta{floatname}|name| macros redefined locally (AS).}
%    \begin{macrocode}
\newcommand\floatname[2]{\@namedef{#1name}{#2}}
\newcommand\floatplacement[2]{\@namedef{fps@#1}{#2}}
